{
  "name": "api",
  "root": "apps/api",
  "sourceRoot": "apps/api",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun build index.ts --outdir ../../dist/apps/api",
        "cwd": "apps/api"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run index.ts",
        "cwd": "apps/api"
      }
    }
  }
}
